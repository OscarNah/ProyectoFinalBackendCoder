<div class="wrapper">
<form action="/api/users/register" method="post" class="register-form-container">
    <div class="register-form-group">
        <label for="first_name" class="register-form-label">First Name:</label>
        <input type="text" id="first_name" name="first_name" class="register-form-input" required>
    </div>
    <div class="register-form-group">
        <label for="last_name" class="register-form-label">Last Name:</label>
        <input type="text" id="last_name" name="last_name" class="register-form-input" required>
    </div>
    <div class="register-form-group">
        <label for="email" class="register-form-label">Email:</label>
        <input type="email" id="email" name="email" class="register-form-input" required>
    </div>
    <div class="register-form-group">
        <label for="password" class="register-form-label">Password:</label>
        <input type="password" id="password" name="password" class="register-form-input" required>
    </div>
    <div class="register-form-group">
        <label for="age" class="register-form-label">Age:</label>
        <input type="number" id="age" name="age" class="register-form-input" required>
    </div>
    <button type="submit" class="register-form-button">Register</button>
</form>
</div>
